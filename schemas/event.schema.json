{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "id",
    "kind",
    "payload",
    "created_at"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "kind": {
      "enum": [
        "graph_patch",
        "policy",
        "rollback"
      ]
    },
    "payload": {
      "type": "object"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}